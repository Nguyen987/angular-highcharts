{
  "name": "unity-angular-highcharts",
  "version": "0.1.0",
  "description": "Highcharts wrapper for Angular 1.x",
  "author": "Maciej Gurban <maciej@unity3d.com>",
  "main": "dist/angular-highcharts.js",
  "scripts": {
    "test": "karma start ./karma.conf.js",
    "build": "browserify src/angular-highcharts.js -s angular-highcharts | derequire > dist/angular-highcharts.js && npm run build-demo",
    "build-demo": "browserify demo/app.js > demo/app.final.js",
    "watch": "watchify src/angular-highcharts.js -o dist/angular-highcharts.js",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "angular": "1.5.9",
    "highcharts": "4.2.0"
  },
  "devDependencies": {
    "angular-mocks": "1.5.9",
    "babel-cli": "6.10.1",
    "babel-preset-es2015": "6.9.0",
    "babelify": "7.3.0",
    "browserify": "13.1.0",
    "browserify-ngannotate": "2.0.0",
    "derequire": "^2.0.6",
    "jasmine-core": "2.2.0",
    "karma": "0.12.37",
    "karma-babel-preprocessor": "6.0.1",
    "karma-browserify": "5.1.1",
    "karma-cli": "1.0.1",
    "karma-jasmine": "0.3.8",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-spec-reporter": "0.0.26",
    "phantomjs-prebuilt": "2.1.14",
    "watchify": "3.7.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-ngannotate"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "private": true
}
